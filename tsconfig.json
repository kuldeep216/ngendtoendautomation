{
  "compilerOptions": {
   
    "target": "es6",                   
    "module": "commonjs", 
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "declaration": false,
    "noImplicitAny": false,   
    "sourceMap": false,  
    "removeComments": false,
    
    "outDir": "JSFiles",

    "esModuleInterop": true,
    "skipLibCheck": true,
    //"forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
